{
  "name": "mc-player-admin-service",
  "version": "0.0.0",
  "description": "mc玩家管理系统-后端",
  "main": "src/app.ts",
  "repository": "https://github.com/mc-player-admin/service",
  "author": "ltfei <ltfei@ltfei.cn> & alongw<alongw@alongw.cn>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "cz": "git-cz",
    "format": "prettier --write --cache .",
    "start": "yarn start:ts",
    "start:ts": "ts-node-esm --experimental-specifier-resolution=node src/app.ts",
    "start:node": "node --es-module-specifier-resolution=node dist/app.js",
    "dev": "nodemon -x node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/app.ts",
    "build": "tsc"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-angular": "^17.6.5",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express-jwt": "6.0.4",
    "@types/jsonwebtoken": "8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.5",
    "@types/nodemailer": "^6.4.8",
    "commitizen": "^4.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-jwt": "5.3.3",
    "jsonwebtoken": "^9.0.0",
    "log4js": "^6.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.3",
    "nodemailer": "^6.9.3"
  }
}
